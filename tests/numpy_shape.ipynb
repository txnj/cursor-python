    "import numpy as np\n",
    "\n",
    "# 创建一个一维数组\n",
    "arr1 = np.array([1, 2, 3, 4])\n",
    "print(arr1.shape)  # 输出: (4,)\n",
    "\n",
    "# 创建一个二维数组\n",
    "arr2 = np.array([[1, 2, 3], [4, 5, 6]])\n",
    "print(arr2.shape[0])  # 输出: (2, 3)\n",
    "\n",
    "# 创建一个三维数组\n",
    "arr3 = np.array([[[1, 2], [3, 4]], [[5, 6], [7, 8]]])\n",
    "print(arr3.shape)  # 输出: (2, 2, 2)"
   ]
  }
